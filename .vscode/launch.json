    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) dvmdostem",
            "type": "cppdbg",
            "request": "launch",
            "program": "/work/dvmdostem",
            "args": ["-l", "fatal", "-p", "0", "-e", "0", "-s", "0", "-t", "2", "-n", "0", "--restart-run"],
            "stopAtEntry": false,
            "cwd": "/data/workflows/thermokarst/init",
